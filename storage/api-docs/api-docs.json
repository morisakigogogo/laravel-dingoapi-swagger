{
    "openapi": "3.0.0",
    "info": {
        "title": "laravel + dingoApi デモ",
        "contact": {
            "name": "hirose.morisaki",
            "url": "/",
            "email": "hirose.morisaki@gmail.com"
        },
        "version": "1.0"
    },
    "servers": [
        {
            "url": "http://shopapi.test/api"
        }
    ],
    "paths": {
        "/admin/users": {
            "get": {
                "tags": [
                    "Admin"
                ],
                "summary": "会員リスト",
                "description": "会員リスト",
                "operationId": "users",
                "parameters": [
                    {
                        "name": "name",
                        "in": "query",
                        "description": "name",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "email",
                        "in": "query",
                        "description": "Email",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "200 成功しました"
                    },
                    "400": {
                        "description": "400 エラーです"
                    }
                },
                "deprecated": false
            }
        },
        "/admin/users/{id}": {
            "get": {
                "tags": [
                    "Admin"
                ],
                "summary": "会員詳細",
                "description": "会員詳細",
                "operationId": "users/1",
                "responses": {
                    "200": {
                        "description": ""
                    },
                    "400": {
                        "description": "エラーです。"
                    }
                },
                "deprecated": false
            }
        },
        "/auth/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "ログイン",
                "description": "会員ログイン",
                "operationId": "login",
                "parameters": [
                    {
                        "name": "email",
                        "in": "query",
                        "description": "E-mail",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "password",
                        "in": "query",
                        "description": "パスワード",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "ログインしました！"
                    },
                    "400": {
                        "description": "ログインエラーです。"
                    }
                },
                "deprecated": false
            }
        },
        "/auth/logout": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "ログアウト",
                "description": "会員ログアウト",
                "operationId": "logout",
                "responses": {
                    "200": {
                        "description": "ログアウトしました！"
                    },
                    "400": {
                        "description": "ログアウトエラーです。"
                    }
                },
                "deprecated": false
            }
        }
    },
    "components": {
        "securitySchemes": {
            "access_token": {
                "type": "apiKey",
                "description": "Use a global client_id / client_secret and your email / password combo to obtain a token",
                "name": "access_token",
                "in": "header",
                "flows": {
                    "password": {
                        "authorizationUrl": "/oauth/authorize",
                        "tokenUrl": "/oauth/token",
                        "refreshUrl": "/oauth/token/refresh",
                        "scopes": {}
                    }
                },
                "scheme": "http"
            },
            "passport": {
                "type": "oauth2",
                "description": "Laravel passport oauth2 security.",
                "in": "header",
                "scheme": "https",
                "flows": {
                    "password": {
                        "authorizationUrl": "http://localhost/oauth/authorize",
                        "tokenUrl": "http://localhost/oauth/token",
                        "refreshUrl": "http://localhost/token/refresh",
                        "scopes": []
                    }
                }
            }
        }
    },
    "security": [
        []
    ]
}